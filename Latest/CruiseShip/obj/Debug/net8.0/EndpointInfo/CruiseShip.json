  "components": {
    "schemas": {
      "Booking": {
        "required": [
          "endTime",
          "startTime",
          "status"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "voyagerId": {
            "type": "string",
            "nullable": true
          },
          "facilityId": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
